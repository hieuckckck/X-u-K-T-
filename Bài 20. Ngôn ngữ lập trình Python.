#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    string s;
    getline(cin,s);
    string t="python";
    int idx=0;
    for(char x:s){
        if(x==t[idx]){
            idx++;
        }
        if(idx==6){
            cout<<"YES";
            return 0;
        }
    }
    cout<<"NO";
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
