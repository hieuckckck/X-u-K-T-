#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    string s;
    getline(cin,s);int sum=0;
    for(int i=0;i<s.length();i++){
        sum=sum+s[i]-'0';
}
    if(sum%3==0&&((s[s.length()-1]-'0')%5==0)){
        cout<<"YES";
    }
    else cout<<"NO";
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
